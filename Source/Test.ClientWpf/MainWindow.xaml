<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Test.ClientWpf" x:Class="Test.ClientWpf.MainWindow"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Window.DataContext>
        <local:MainWindowModel/>
    </Window.DataContext>
    <Grid>
        <Button Content="测试" Command="{Binding BtnClick}" CommandParameter="2" HorizontalAlignment="Left" Height="112.5" Margin="10,10,0,0" VerticalAlignment="Top" Width="165" />
        <Label Content="{Binding LabTxt}" HorizontalAlignment="Left" Margin="180,10,0,0" VerticalAlignment="Top" Height="94" Width="417"/>
    </Grid>
</Window>
