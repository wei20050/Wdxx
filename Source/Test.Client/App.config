<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0"/>
    <supportedRuntime version="v2.0.50727"/>
  </startup>
  <connectionStrings>
    <!--是否记录数据库日志(0:不记录 1:记录)-->
    <add name="SqlLog" connectionString="1"/>
    <!--离线数据库地址-->
    <add name="DbContext" connectionString="Data Source=Data\mydb.db;" providerName="System.Data.SQLite"/>
  </connectionStrings>
</configuration>
